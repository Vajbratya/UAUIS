{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_DATABASE_URL": "@database_url",
    "VITE_CLAUDE_API_KEY": "@claude_api_key"
  }
}
